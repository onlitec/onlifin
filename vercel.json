{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3",
      "excludeFiles": [
        "node_modules/**",
        "tests/**",
        "vendor/bin/**",
        "vendor/phpunit/**",
        "vendor/phpspec/**",
        "vendor/sebastian/**",
        "vendor/mockery/**",
        "vendor/doctrine/instantiator/**",
        "vendor/doctrine/deprecations/**",
        "vendor/psy/psysh/**",
        "vendor/filp/whoops/**",
        "vendor/nunomaduro/collision/**",
        "storage/logs/**",
        "storage/debugbar/**",
        "storage/framework/cache/**",
        "storage/framework/sessions/**",
        "storage/framework/views/**",
        "*.md",
        "*.txt",
        "*.log",
        "phpunit.xml",
        "phpstan.neon",
        ".*"
      ]
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_NAME": "Onlifin",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://onlifin.vercel.app",
    "DB_CONNECTION": "sqlite",
    "DB_DATABASE": "/var/task/database/database.sqlite",
    "CACHE_STORE": "file",
    "SESSION_DRIVER": "file",
    "QUEUE_CONNECTION": "sync",
    "MAIL_MAILER": "log"
  },
  "buildCommand": "chmod +x vercel-build.sh && ./vercel-build.sh",
  "outputDirectory": "public",
  "framework": null
}